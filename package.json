{
  "name": "codex-agent-workflow-installer",
  "version": "1.0.0",
  "private": false,
  "description": "One-command installer for Codex/Trae/Cursor multi-agent workflow",
  "bin": {
    "codex-agent-workflow-install": "bin/install.js"
  },
  "scripts": {
    "install:local": "node bin/install.js --ide codex",
